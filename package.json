{
  "name": "livedate",
  "version": "0.5.0",
  "description": "Visual date picker script",
  "main": "src/livedate.js",
  "scripts": {
    "build": "mkdirp build && (uglifyjs --compress booleans  --mangle --source-map build/livedate.min.js.map -- src/livedate.js > build/livedate.min.js & node-sass --output-style expanded src/livedate.sass > build/livedate.css)",
    "watch": "watch \"npm run build\" src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://live627@github.com/live627/livedate.git"
  },
  "keywords": [
    "date",
    "picker",
    "datepicker",
    "calendar"
  ],
  "author": "live627 <live627@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/live627/livedate/issues"
  },
  "homepage": "https://github.com/live627/livedate",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "less": "^2.5.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.3.2",
    "uglify-js": "^2.4.24",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "element-closest": "^1.0.3"
  }
}
